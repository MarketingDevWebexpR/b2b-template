    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules/expo-updates/bin/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules/expo-updates/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules/expo-updates/bin/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules/expo-updates/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/expo-updates@0.26.19_expo@52.0.47_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.2_y6ub6hpsnni4kh4vsmmvntjfm4/node_modules:/Users/jean_webexpr/Documents/projets_webexpr/monorepo_bijoux_B2B/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo-updates/bin/cli.js" "$@"
else
  exec node  "$basedir/../expo-updates/bin/cli.js" "$@"
fi
