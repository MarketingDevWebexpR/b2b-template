{
  "expo": {
    "name": "Maison Bijoux",
    "slug": "bijoux-sage",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "maisonbijoux",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffcf7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maisonbijoux.app",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Maison Bijoux utilise la camera pour scanner les codes-barres et QR codes de vos bijoux preferes.",
        "NSMicrophoneUsageDescription": "Maison Bijoux utilise le microphone pour la recherche vocale de produits.",
        "NSPhotoLibraryUsageDescription": "Maison Bijoux utilise votre phototeque pour partager vos bijoux preferes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:maisonbijoux.com",
        "applinks:www.maisonbijoux.com"
      ],
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fffcf7"
      },
      "package": "com.maisonbijoux.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Inter-Bold.ttf",
            "./assets/fonts/PlayfairDisplay-Regular.ttf",
            "./assets/fonts/PlayfairDisplay-Medium.ttf",
            "./assets/fonts/PlayfairDisplay-SemiBold.ttf",
            "./assets/fonts/PlayfairDisplay-Bold.ttf",
            "./assets/fonts/CormorantGaramond-Regular.ttf",
            "./assets/fonts/CormorantGaramond-Medium.ttf",
            "./assets/fonts/CormorantGaramond-SemiBold.ttf",
            "./assets/fonts/CormorantGaramond-Bold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Maison Bijoux utilise la camera pour scanner les codes-barres et QR codes de vos bijoux preferes.",
          "microphonePermission": "Maison Bijoux utilise le microphone pour la recherche vocale.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": false,
          "enableApplePay": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "webexpr",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1c5734c3-91c6-43e2-9287-91496a3d3188"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/1c5734c3-91c6-43e2-9287-91496a3d3188"
    }
  }
}
