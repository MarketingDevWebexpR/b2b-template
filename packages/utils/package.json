{
  "name": "@maison/utils",
  "version": "0.0.0",
  "description": "Shared utility functions for Maison packages",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarketingDevWebexpR/monorepo_bijoux.git",
    "directory": "packages/utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@maison/types": "workspace:*",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@maison/types": "workspace:*"
  }
}
