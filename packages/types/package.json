{
  "name": "@maison/types",
  "version": "0.0.0",
  "description": "Shared TypeScript types for Maison packages",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarketingDevWebexpR/monorepo_bijoux.git",
    "directory": "packages/types"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
